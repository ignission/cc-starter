{
  "name": "@ignission/cc-starter",
  "version": "0.0.1",
  "description": "Claude Code開発のベストプラクティスを凝縮。AIファーストな構成で開発効率を最大化",
  "type": "module",
  "license": "MIT",
  "author": "Ignission",
  "homepage": "https://github.com/ignission/cc-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ignission/cc-starter.git"
  },
  "bugs": {
    "url": "https://github.com/ignission/cc-starter/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "cc-starter": "./bin/cli.js"
  },
  "files": [
    "bin",
    "lib",
    "templates"
  ],
  "engines": {
    "node": ">=22.17.0",
    "pnpm": ">=10.13.1"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run",
    "release": "changeset publish",
    "version": "changeset version",
    "lint": "biome check .",
    "format": "biome format --write .",
    "fix": "biome check --write .",
    "fix:unsafe": "biome check --unsafe --fix .",
    "fix:all": "pnpm format && pnpm fix"
  },
  "dependencies": {
    "chalk": "5.4.1",
    "commander": "14.0.0",
    "execa": "9.6.0",
    "fs-extra": "11.3.0",
    "inquirer": "12.7.0",
    "ora": "8.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@changesets/cli": "2.29.5",
    "@types/fs-extra": "11.0.4",
    "@types/inquirer": "9.0.8",
    "@types/node": "22.16.3",
    "vitest": "3.2.4"
  },
  "keywords": [
    "cli",
    "starter",
    "template",
    "claude",
    "claude-code",
    "react-native",
    "react-router",
    "vite",
    "monorepo",
    "pnpm"
  ]
}